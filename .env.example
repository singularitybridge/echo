# Google Gemini API Key (Required)
# Get your API key from: https://ai.google.dev/
NEXT_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API Key (Optional - for audio transcription in evaluation)
# Get your API key from: https://platform.openai.com/api-keys
NEXT_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# fal.ai API Key (Required for Veo 3.1 video generation and character reference generation)
# Get your API key from: https://fal.ai/dashboard
NEXT_PUBLIC_FAL_KEY=your_fal_api_key_here

# Agent Hub Configuration (Required for AI story generation)
# Get your API key from Agent Hub dashboard
AGENT_HUB_API_URL=http://localhost:3000/assistant
AGENT_HUB_API_KEY=your_agent_hub_api_key_here
